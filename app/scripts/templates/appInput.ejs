
<div class="col-md-8">
		<form class="form-horizontal well inputForm" role="form">

			<div class="form-group">
			    <label for="keywordList" class="col-sm-2 control-label">App Id</label>
			    <div class="col-sm-10">
			      <input type="text" class="form-control" id="appId" placeholder="App Name" value="<%= id %>">
			    </div>
			  </div>

			  <div class="form-group">
			    <label for="keywordList" class="col-sm-2 control-label">App Name</label>
			    <div class="col-sm-10">
			      <input type="text" class="form-control" id="appName" placeholder="App Name" value="<%= name %>">
			    </div>
			  </div>

			  <div class="form-group">
			    <label for="keywordList" class="col-sm-2 control-label">Keywords</label>
			    <div class="col-sm-10">
			      <input type="text" class="form-control" id="keywordList" placeholder="Keywords" value="<%= keywords %>">
			    </div>
			  </div>

			  <div class="form-group">
			    <label for="keywordList" class="col-sm-2 control-label">App Store Name</label>
			    <div class="col-sm-10">
			      <input type="text" class="form-control" id="appStoreName" placeholder="App Store Name" value="<%= track.trackName %>">
			    </div>
			  </div>
	
			<div class="form-group">
			    <label for="keywordList" class="col-sm-2 control-label">Description</label>
			    <div class="col-sm-10">
			      <textarea class="form-control" id="description" placeholder="App Description" rows="5"><%= track.description %></textarea>
			    </div>
			  </div>

			  <div class="form-group">
			    <label for="keywordList" class="col-sm-2 control-label">Notes</label>
			    <div class="col-sm-10">
			      <textarea class="form-control" id="notes" placeholder="Note about this app" rows='5'><%= notes %></textarea>
			    </div>
			  </div>

				<div class="form-group">
					<div class="pull-right">
					  <button type="button" class="btn btn-danger cancel">Cancel</button>
					  <button type="button" class="btn btn-success save">Save</button>
				  </div>
			</div>
		</form>
</div>