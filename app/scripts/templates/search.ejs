<div class="row">
	<div class="col-md-4 well" id="keywordForm">
		<form class="form" role="form">
		<div class="row">
			<div class="col-md-12">
				<div class="input-group">
					<input type="text" class="form-control" id="keywords" placeholder="Enter Keywords">
					<span class="input-group-btn">
						<button class="btn btn-success" id="searchButton">Search</button>
					</span>
				</div>
			</div>
		</div>



		<label class="checkbox-inline text-muted">
		<input type="checkbox" id="descriptions">search descirptions too?
		</label>
	
		</form>
	</div>

	<div class="col-md-offset-1 col-md-7 well" id="keywordListContainer">

			<form class="form-horizontal addMetaToApp" role="form">
			  <div class="form-group">
			    <label for="appsList" class="col-sm-2 control-label">App</label>
			    <div class="col-sm-10">
			     <select class="form-control appsList">
				  <option>New App</option>
				</select>
			    </div>
			  </div>
	
			<div class="form-group">
			    <label for="keywordList" class="col-sm-2 control-label">Name</label>
			    <div class="col-sm-10">
			      <input type="text" class="form-control" id="nameList" placeholder="Keywords">
			    </div>
			  </div>

			  <div class="form-group" style="margin-bottom:0">
			    <label for="keywordList" class="col-sm-2 control-label">Keywords</label>
			    <div class="col-sm-10">
			      <input type="text" class="form-control" id="keywordList" placeholder="Keywords">
			      <span class="help-block" style="margin-bottom:0"><strong>Count:</strong> <span id="charCount"></span>   <strong>Left:</strong> <span id="charLeft"></span></span>
			    </div>
			  </div>
			
			<div class="form-group">
					<div class="pull-right">
					  <button type="button" class="btn btn-danger cancel">Cancel</button>
					  <button type="submit" class="btn btn-success add">Add</button>
				  </div>
			</div>

			</form>

	</div>
</div>
<div class="row keywordResults hide">
	<div class="col-md-6" id="ranks">
		<h3>Ranked Keywords</h3><table id="ranks" class="table table-bordered"></table>
	</div>
	<div class="col-md-6" id="results">
		<h3>Search Results</h3><table id="tracks" class="table table-bordered"></table>
	</div>
</div>
